{
  "name": "PrintaDot",
  "version": "1.0",
  "manifest_version": 3,
  "description": "Browser extension for printing",
  "background": {
    "service_worker": "main.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "printer.png"
  },
 "permissions": [
    "nativeMessaging",
    "activeTab"
  ],
  "host_permissions": [
    "http://localhost:8081/*"
  ],
  "content_scripts": [
    {
      "matches": ["http://localhost:8081/*"],
      "js": ["content-script.js"]
    }
  ]
}